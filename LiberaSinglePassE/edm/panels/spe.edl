  6 "$(HOSTNAME):$(BPM):spe.x"
  7 "$(HOSTNAME):$(BPM):spe.y"
}
ySigned {
  0 1
  1 1
  2 1
  3 1
  4 1
  5 1
  6 1
  7 1
}
plotColor {
  0 index 16
  1 index 83
  2 index 46
  3 index 43
  4 index 14
  5 index 36
  6 index 26
  7 index 21
}
endObjectProperties

